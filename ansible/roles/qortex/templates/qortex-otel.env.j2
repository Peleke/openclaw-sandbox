# {{ ansible_managed }}
# systemd EnvironmentFile format (no 'export' keyword).
# Loaded by openclaw-gateway.service via EnvironmentFile=-
QORTEX_OTEL_ENABLED={{ qortex_otel_enabled | string | lower }}
OTEL_EXPORTER_OTLP_ENDPOINT={{ qortex_otel_endpoint }}
OTEL_EXPORTER_OTLP_PROTOCOL={{ qortex_otel_protocol | default('http/protobuf') }}
QORTEX_PROMETHEUS_ENABLED={{ qortex_prometheus_enabled | default(false) | string | lower }}
QORTEX_PROMETHEUS_PORT={{ qortex_prometheus_port | default(9090) }}
